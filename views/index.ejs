<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PaoTree</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <div class="container">
        <div class="profile">
            <img src="/images/profile-pic.jpg" alt="Profile Picture" class="profile-pic">
            <h1>My Links</h1>
        </div>
        <ul>
            <% links.forEach(link => { %>
                <li>
                    <a href="<%= link.url %>">
                        <img src="/images/<%= link.icon %>" alt="<%= link.text %> Icon">
                        <%= link.text %>
                    </a>
                </li>
            <% }) %>
        </ul>
    </div>
</body>
</html>